{
  "manifest_version": 3,
  "name": "Tailwind AI",
  "description": "Tailwind AI for chrome extension",
  "version": "1.0.0",
  "action": {
    "default_popup": "popup.html",
    "default_title": "Tailwind AI",
    "default_icon": "icon.png"
  },
  "permissions": ["bookmarks", "storage", "identity"],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "options_page": "options.html",
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
        "src_assets_tailwind_css.js",
        "vendors-node_modules_react-dom_client_js.js",
        "contentScript.js"
      ]
    }
  ]
}
